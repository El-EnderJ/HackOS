{
  "name": "rf_433_presets",
  "label": "RF 433 Presets",
  "version": "1.0.0",
  "author": "HackOS Team",
  "description": "Quick RF frequency presets for Sub-GHz work",
  "category": "rf",
  "config": {
    "pin": 25,
    "frequency": 433920000,
    "protocol": "OOK"
  },
  "actions": [
    {
      "type": "freq_set",
      "value": 433920000,
      "label": "Set 433.92 MHz"
    },
    {
      "type": "freq_set",
      "value": 315000000,
      "label": "Set 315 MHz"
    },
    {
      "type": "freq_set",
      "value": 868000000,
      "label": "Set 868 MHz"
    },
    {
      "type": "gpio_toggle",
      "pin": 25,
      "label": "Toggle TX Pin (GPIO25)"
    },
    {
      "type": "log_msg",
      "label": "Log RF Status"
    }
  ]
}
